{
  "version": "0.1.2",
  "name": "github-syntax-theme-generator",
  "description": "The GitHub syntax theme generator will generate themes for multiple platforms based on the GitHub syntax theme.",
  "homepage": "https://github.com/primer/github-syntax-theme-generator",
  "author": "GitHub, Inc.",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "build"
  ],
  "repository": "https://github.com/primer/github-syntax-theme-generator.git",
  "bugs": {
    "url": "https://github.com/primer/github-syntax-theme-generator/issues"
  },
  "scripts": {
    "setup": "if [ ! -d \"node_modules\" ]; then npm install; fi",
    "build": "node index.js",
    "prepublish": "npm run setup && npm run build",
    "ava": "ava --verbose --fail-fast \"test/**/*.js\"",
    "test": "npm run build && npm run ava"
  },
  "devDependencies": {
    "ace": "git+https://github.com/ajaxorg/ace.git",
    "atom-package-manager": "^1.0.0",
    "ava": "^0.15.2",
    "colorable": "^1.0.5",
    "css-parse": "2.0.0",
    "css-stringify": "2.0.0",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "plist": "^1.2.0",
    "postcss": "^5.0.21",
    "postcss-discard-empty": "^2.1.0",
    "postcss-merge-selectors": "0.0.6"
  },
  "keywords": [
    "syntax",
    "github",
    "css",
    "theme",
    "code",
    "highlight",
    "generator"
  ]
}
